{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "TANHAPON TOSIRIKUL",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --quiet",
    "build": "yarn tsc",
    "test": "dotenv -e .env.test jest",
    "migrate-test": "dotenv -e .env.test -- yarn run prisma migrate dev --name init",
    "reset-test": "dotenv -e .env.test -- yarn run prisma migrate reset --force",
    "migrate": "dotenv -e .env.development -- yarn run prisma migrate dev --name init",
    "reset": "dotenv -e .env.development -- yarn run prisma migrate reset --force",
    "start": "dotenv -e .env.development -- nodemon src/index.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^17.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "^3.6.0",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@prisma/client": "^3.6.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "supertest": "^6.1.6"
  }
}
